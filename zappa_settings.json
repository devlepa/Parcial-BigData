{
    "download-dev": {
        "app_function": "main.lambda_handler_download_scrape",
        "aws_region": "us-east-1",
        "exclude": [
            "boto3",
            "botocore",
            "s3transfer",
            "concurrent",
            "dateutil"
        ],
        "project_name": "parcial1-download",
        "runtime": "python3.12",
        "s3_bucket": "zappa-xe6o93rce",
        "apigateway_enabled": false,
        "manage_roles": false,
        "role_name": "LabRole",
        "slim_handler": true,
        "events": [
            {
                "function": "main.lambda_handler_download_scrape",
                "expression": "cron(0 9 ? * MON *)"
            }
        ],
        "use_environment_credentials": true
    },
    "upload-dev": {
        "app_function": "main.lambda_handler_upload_csv",
        "aws_region": "us-east-1",
        "exclude": [
            "boto3",
            "botocore",
            "s3transfer",
            "concurrent",
            "dateutil"
        ],
        "project_name": "parcial1-upload",
        "runtime": "python3.12",
        "s3_bucket": "zappa-xe6o93rce",
        "apigateway_enabled": false,
        "manage_roles": false,
        "role_name": "LabRole",
        "slim_handler": true,
        "events": [
            {
                "function": "main.lambda_handler_upload_csv",
                "expression": "cron(30 9 ? * MON *)"
            }
        ],
        "use_environment_credentials": true
    }
}
